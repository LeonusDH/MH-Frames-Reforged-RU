<?xml version="1.0" encoding="utf-8" ?>
<Patch>
	<!-- ResearchProject Patch -->
	<Operation Class="PatchOperationFindMod">
		<mods>
			<li>MH: Industrial Automatons</li>
		</mods>
		<match Class="PatchOperationSequence">
			<operations>
				<!-- ATR_VacuumFrameworks -->
				<li Class="PatchOperationReplace">
					<xpath>Defs/ResearchProjectDef[defName = "ATR_VacuumFrameworks"]/researchViewX</xpath>
					<value><researchViewX>9.00</researchViewX></value></li>
				<li Class="PatchOperationReplace">
					<xpath>Defs/ResearchProjectDef[defName = "ATR_VacuumFrameworks"]/researchViewY</xpath>
					<value><researchViewY>2.40</researchViewY></value></li>
				<li Class="PatchOperationAdd">
					<xpath>Defs/ResearchProjectDef[defName = "ATR_VacuumFrameworks"]/prerequisites</xpath>
					<value><li>ATR_AdvancedIndustrialFrames</li></value></li>
			</operations>
		</match>
	</Operation>

</Patch>
